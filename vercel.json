{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": [
        "api/transfers_snapshot_2026-01-16.json"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}
